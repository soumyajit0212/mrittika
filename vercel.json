{
  "version": 2,
  "buildCommand": "npm ci && npm run build",
  "outputDirectory": "dist",
  "framework": null,
  "functions": {
    "api/**/*.ts": { "runtime": "nodejs20.x" },
    "api/**/*.js": { "runtime": "nodejs20.x" }
  },
  "routes": [
    { "src": "/api/trpc(?:/.*)?", "dest": "/api/trpc/[trpc].ts" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
