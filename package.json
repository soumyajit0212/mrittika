{
  "name": "first-project",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@9.12.3",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "prebuild": "node -e \"require('fs').mkdirSync('src/generated', { recursive: true })\" && tsr generate || true",
    "postinstall": "prisma generate",
    "dev": "vinxi dev",
    "build": "prisma generate && vinxi build",
    "vercel-build": "prisma generate && vinxi build",
    "start": "vinxi preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . -c eslint.config.mjs --max-warnings 0",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@headlessui/react": "^2.2.3",
    "@hookform/resolvers": "^5.1.1",
    "@prisma/client": "^6.13.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "5.x",
    "@tanstack/react-router": "^1.119.0",
    "@tanstack/react-start": "^1.119.0",
    "@tanstack/router-cli": "^1.119.0",
    "@tanstack/router-plugin": "^1.119.0",
    "@vitejs/plugin-react": "4.7.0",
    "@trpc/client": "11.4.4",
    "@trpc/react-query": "11.4.4",
    "@trpc/server": "11.4.4",
    "autoprefixer": "^10.4.20",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.532.0",
    "minio": "^8.0.5",
    "postcss": "^8.5.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.58.1",
    "react-hot-toast": "^2.5.2",
    "redaxios": "^0.5.1",
    "superjson": "^2.2.2",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.4",
    "vinxi": "0.5.8",
    "vite": "^7.1.1",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^3.25.0",
    "zustand": "^5.0.6",
    "prisma": "^6.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@tanstack/eslint-plugin-router": "^1.115.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.5.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "4.7.0",
    "eslint": "^9.25.1",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "node-stdlib-browser": "^1.3.1",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.31.1",
    "unenv": "^1.10.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
